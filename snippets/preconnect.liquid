<link rel="modulepreload" href="{{ 'component-header.js' | asset_url }}">
<link rel="modulepreload" href="{{ 'component-details-modal.js' | asset_url }}">
<link rel="modulepreload" href="{{ 'component-sticky-header.js' | asset_url }}">

<link rel="preconnect" href="https://cdn.shopify.com" crossorigin />

{%- unless settings.type_body_font.system? -%}
  <link
    rel="preload"
    as="font"
    href="{{ settings.type_body_font | font_url }}"
    type="font/woff2"
    crossorigin
  />
  {%- endunless -%} {%- unless settings.type_header_font.system? -%}
  <link
    rel="preload"
    as="font"
    href="{{ settings.type_header_font | font_url }}"
    type="font/woff2"
    crossorigin
  />
{%- endunless -%}

{%- unless settings.type_header_font.system? and settings.type_body_font.system? -%}
  <link rel="preconnect" href="https://fonts.shopifycdn.com" crossorigin />
{%- endunless -%}
